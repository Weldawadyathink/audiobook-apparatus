{
  "imports": {
    "@hono/trpc-server": "npm:@hono/trpc-server@^0.3.2",
    "@trpc/client": "npm:@trpc/client@^10.45.2",
    "@trpc/server": "npm:@trpc/server@^10.45.2",
    "hono": "npm:hono@^4.6.3",
    "zod": "npm:zod@^3.23.8"
  },
  "tasks": {
    "dev": "deno serve --watch --port 8000 --allow-read --allow-net --allow-env main.ts",
    "serve": "deno serve --port 8000 --no-check --allow-read --allow-net --allow-env main.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}
